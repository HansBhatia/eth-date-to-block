{
  "name": "ethereum-block-to-date",
  "version": "0.0.0-development",
  "description": "An npm package to efficiently convert date formats to Ethereum block numbers",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "test": "jest",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kai-thompson/ethereum-date-to-block.git"
  },
  "keywords": [
    "npm",
    "javascript",
    "typescript",
    "esm",
    "cjs",
    "nodejs",
    "commonjs",
    "ecmascript",
    "blockchain",
    "ethereum",
    "eth",
    "date-converter"
  ],
  "author": "kai-thompson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kai-thompson/ethereum-date-to-block/issues"
  },
  "homepage": "https://github.com/kai-thompson/ethereum-date-to-block#readme",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "ethers": "^5.7.1",
    "jest": "^29.1.2",
    "semantic-release": "^19.0.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "web3": "^1.8.0"
  }
}
